num = 5
if num > 0:
    num = num + 10
print (num)


num = -7
if num > 0:
    num = num + 10
print (num)

num = 3
number_is_positive = False
if num > 0:
    num = num + 10
    number_is_positive = True
print(num)
print(number_is_positive)

name = 'Jennifer'
if name == 'Jennifer':
    print ("your name is Jennifer. ")

if name == 'Bob':
    print ("your name is Bob. ")

num = 12
if num > 10:
    print (num, "Greater than 10. ")
if num < -4:
    print (num, "Less than -4. ")
    
