percentage = int (input ("What is your percentage? "))
if percentage > 90:
    print ("You got an A")
elif percentage > 80:
    print("You got a B")
elif percentage > 70:
    print("You got a C")
elif percentage > 60:
    print("You got a D")
else:
    print("You got a F")


age = int (input("What is your age? "))
if age > 18:
    print("You can vote")
if age >= 21:
    print("You can drink Alcohol")
else:
    print ("You can't vote and drink Alcohol ")
